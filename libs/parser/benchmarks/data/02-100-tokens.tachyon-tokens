IDENT "add" EQ FN LPAREN IDENT "a" COMMA IDENT "b" RPAREN LBRACE
RETURN IDENT "a" PLUS IDENT "b" SEMIC RBRACE NLINE
IDENT "x" EQ NUMBER 10 NLINE
IDENT "y" EQ NUMBER 20 NLINE
IDENT "sum" EQ IDENT "add" LPAREN IDENT "x" COMMA IDENT "y" RPAREN NLINE
IDENT "prod" EQ IDENT "x" STAR IDENT "y" NLINE
IDENT "div" EQ IDENT "y" FSLASH IDENT "x" NLINE
IDENT "exp" EQ IDENT "x" CARET NUMBER 2 NLINE
IDENT "cmp1" EQ IDENT "x" ECOMP IDENT "y" NLINE
IDENT "cmp2" EQ IDENT "x" NECOMP IDENT "y" NLINE
IDENT "cmp3" EQ IDENT "x" LCOMP IDENT "y" NLINE
IDENT "cmp4" EQ IDENT "x" GCOMP IDENT "y" NLINE
IDENT "cmp5" EQ IDENT "x" LECOMP IDENT "y" NLINE
IDENT "cmp6" EQ IDENT "x" GECOMP IDENT "y" NLINE
IDENT "not_x" EQ NOT IDENT "x" NLINE
IDENT "and_xy" EQ IDENT "x" BAND IDENT "y" NLINE
IDENT "or_xy" EQ IDENT "x" BOR IDENT "y" NLINE
IDENT "i" EQ NUMBER 0 NLINE
WHILE IDENT "i" LCOMP NUMBER 5 LBRACE NLINE
IDENT "i" EQ IDENT "i" PLUS NUMBER 1 SEMIC NLINE
RBRACE NLINE
IDENT "unit_val" EQ UNIT NLINE
IDENT "flag" EQ BOOL True NLINE
IDENT "flag2" EQ BOOL False NLINE
END